ui = true

storage "consul" {
  address = "{{ vault_consul_address }}"
  path    = "{{ vault_consul_path }}"
}

listener "tcp" {
  address     = "0.0.0.0:{{ vault_api_port }}"
  tls_disable = 1
}

api_addr     = "{{ vault_api_addr }}"
cluster_addr = "http://{{ vault_cluster_addr }}:8201"

disable_mlock = true
